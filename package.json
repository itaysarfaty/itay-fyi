{
    "name": "itay-fyi",
    "version": "1.0.0",
    "description": "A blank template to get started with Payload 3.0",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
        "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
        "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
        "production": "payload migrate && pnpm build",
        "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
        "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
        "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
        "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
        "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
        "db:seed": "tsx ./src/collections/seed.ts",
        "format": "prettier --write \"./**/*.{tsx,ts,css,html}\""
    },
    "dependencies": {
        "@payloadcms/db-sqlite": "3.49.1",
        "@payloadcms/next": "3.49.1",
        "@payloadcms/payload-cloud": "3.49.1",
        "@payloadcms/richtext-lexical": "3.49.1",
        "@payloadcms/storage-vercel-blob": "3.49.1",
        "@radix-ui/react-accessible-icon": "^1.1.2",
        "@tailwindcss/container-queries": "^0.1.1",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "clsx": "^2.1.1",
        "cross-env": "^7.0.3",
        "date-fns": "^4.1.0",
        "dotenv": "^16.4.7",
        "graphql": "^16.10.0",
        "html-to-image": "^1.11.13",
        "libsql": "0.5.4",
        "lucide-react": "^0.487.0",
        "motion": "^12.6.3",
        "next": "15.4.5",
        "next-themes": "^0.4.6",
        "next-view-transitions": "^0.3.4",
        "payload": "3.49.1",
        "prettier-plugin-classnames": "^0.7.3",
        "prettier-plugin-merge": "^0.7.1",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-parallax-tilt": "^1.7.289",
        "sharp": "0.34.0",
        "tailwind-merge": "^2.5.2",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@tailwindcss/postcss": "^4.1.11",
        "@types/node": "^22.14.0",
        "@types/react": "19.1.0",
        "@types/react-dom": "19.1.1",
        "eslint": "^9",
        "eslint-config-next": "15.2.4",
        "postcss": "^8.5.3",
        "tailwindcss": "^4.1.11",
        "typescript": "5.8.3"
    },
    "engines": {
        "node": "^18.20.2 || >=20.9.0",
        "pnpm": ">=9.0.0"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "sharp"
        ],
        "ignoredBuiltDependencies": [
            "esbuild"
        ]
    }
}
