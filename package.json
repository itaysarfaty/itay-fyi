{
    "name": "itay-fyi",
    "version": "1.0.0",
    "description": "A blank template to get started with Payload 3.0",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
        "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
        "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
        "production": "payload migrate && pnpm build",
        "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
        "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
        "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
        "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
        "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
        "db:seed": "tsx ./src/lib/seed.ts",
        "format": "prettier --write \"./**/*.{tsx,ts,css,html}\""
    },
    "dependencies": {
        "@libsql/client": "^0.10.0",
        "@payloadcms/db-sqlite": "beta",
        "@payloadcms/next": "beta",
        "@payloadcms/plugin-cloud": "beta",
        "@payloadcms/richtext-lexical": "beta",
        "@tailwindcss/container-queries": "^0.1.1",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "clsx": "^2.1.1",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.5",
        "framer-motion": "^11.5.4",
        "graphql": "^16.8.1",
        "html-to-image": "^1.11.11",
        "libsql": "0.4.2-pre.3",
        "lucide-react": "^0.438.0",
        "next": "15.0.0-canary.104",
        "payload": "beta",
        "prettier-plugin-classnames": "^0.7.3",
        "prettier-plugin-merge": "^0.7.1",
        "prettier-plugin-tailwindcss": "^0.6.6",
        "react": "19.0.0-rc-06d0b89e-20240801",
        "react-dom": "19.0.0-rc-06d0b89e-20240801",
        "sharp": "0.32.6",
        "tailwind-merge": "^2.5.2",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/node": "^20.14.9",
        "@types/react": "npm:types-react@19.0.0-rc.0",
        "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0",
        "autoprefixer": "^10.4.20",
        "eslint": "^8",
        "eslint-config-next": "15.0.0-canary.104",
        "postcss": "^8.4.44",
        "tailwindcss": "^3.4.10",
        "typescript": "5.5.4"
    },
    "engines": {
        "node": "^18.20.2 || >=20.9.0"
    },
    "pnpm": {
        "overrides": {
            "@types/react": "npm:types-react@19.0.0-rc.0",
            "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0"
        }
    },
    "overrides": {
        "@types/react": "npm:types-react@19.0.0-rc.0",
        "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0"
    }
}
