{
    "name": "itay-fyi",
    "version": "1.0.0",
    "description": "A blank template to get started with Payload 3.0",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
        "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
        "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
        "production": "payload migrate && pnpm build",
        "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
        "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
        "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
        "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
        "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
        "db:seed": "tsx ./src/collections/seed.ts",
        "format": "prettier --write \"./**/*.{tsx,ts,css,html}\""
    },
    "dependencies": {
        "@payloadcms/db-sqlite": "3.55.0",
        "@payloadcms/next": "3.55.0",
        "@payloadcms/payload-cloud": "3.55.0",
        "@payloadcms/richtext-lexical": "3.55.0",
        "@payloadcms/storage-vercel-blob": "3.55.0",
        "@radix-ui/react-accessible-icon": "^1.1.7",
        "@tailwindcss/container-queries": "^0.1.1",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "clsx": "^2.1.1",
        "cross-env": "^10.0.0",
        "date-fns": "^4.1.0",
        "dotenv": "^17.2.2",
        "graphql": "^16.11.0",
        "html-to-image": "^1.11.13",
        "libsql": "0.5.22",
        "lucide-react": "^0.543.0",
        "motion": "^12.23.12",
        "next": "15.5.2",
        "next-themes": "^0.4.6",
        "next-view-transitions": "^0.3.4",
        "payload": "3.55.0",
        "prettier-plugin-classnames": "^0.8.3",
        "prettier-plugin-merge": "^0.8.0",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "react": "19.1.1",
        "react-dom": "19.1.1",
        "react-parallax-tilt": "^1.7.308",
        "sharp": "0.34.3",
        "tailwind-merge": "^3.3.1",
        "zod": "^4.1.5"
    },
    "devDependencies": {
        "@tailwindcss/postcss": "^4.1.13",
        "@types/node": "^24.3.1",
        "@types/react": "19.1.12",
        "@types/react-dom": "19.1.9",
        "eslint": "^9.35.0",
        "eslint-config-next": "15.5.2",
        "postcss": "^8.5.6",
        "tailwindcss": "^4.1.13",
        "typescript": "5.9.2"
    },
    "engines": {
        "node": "^18.20.2 || >=20.9.0",
        "pnpm": ">=9.0.0"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@tailwindcss/oxide",
            "sharp"
        ],
        "ignoredBuiltDependencies": [
            "esbuild"
        ]
    }
}
